#include <stdio.h>

int main()
{
        int n,t1=0,t2=1,next_term;
        next_term=t1+t2;
        printf("Enter the number of terms: ");
        scanf("%d",&n);
        printf("The fibonacci series is: %d,%d,",t1,t2);
        int i;
        for(i=3;i<=n;++i){
                printf("%d, ",next_term);
                t1=t2;
                t2=next_term;
                next_term=t1+t2;
        }
        return 0;
}
