#include <stdio.h>

int main()
{
        char c;
        printf("Enter a single character: ");
        scanf("%c",&c);
        if (c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='A'||c=='E'||c=='I'||c=='O'||c=='U')
        {
                printf("\nThe given character is a vowel.");
        }
        else if (c>='0' && c<='9')
        {
                printf("\nThe given character is a digit.");
        }
        else
        {
                printf("\nThe given character is a consonant.");
        }
        return 0;
}
