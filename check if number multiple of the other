#include <stdio.h>

int main()
{
      int a,b;
      printf("Enter two integers: ");
      scanf("%d%d",&a,&b);
      if(a%b==0)
        printf("%d is a multiple of %d",a,b);
      else if(b%a==0)
        printf("%d is a multiple of %d",b,a);
      else
        printf("Neither is a multiple of the other.");
      return 0;
}
