#include <stdio.h>
#include <math.h>

int main()
{
        int n,prime=0;
        printf("Enter an integer: ");
        scanf("%d",&n);
        if (n<=1){
                printf("Not a prime number.");
        return 0;}
        int i;
        for(i=2;i<=sqrt(n);++i){
                if(n%i==0){
                    prime++;
                    break;
                }
        }

        if(prime==0)
                printf("Prime number.");
        else
                printf("Not a prime number.");
        return 0;
}
