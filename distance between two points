#include <stdio.h>
#include <math.h>

int main()
{
        int x1,y1,x2,y2;
        printf("Enter the coordinates of the first point: ");
        scanf("%d%d",&x1,&y1);
        printf("Enter the coordinates of the second point: ");
        scanf("%d%d",&x2,&y2);
        int diff1,diff2;
        diff1=(x2-x1);
        diff2=(y2-y1);
        float result;
        result=sqrt(pow(diff1,2)+pow(diff2,2));
        printf("\nThe distance is: %f",result);
        return 0;
}
